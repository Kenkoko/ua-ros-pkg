<launch>
    <!-- Load laser tilt controller parameters from YAML file -->
    <rosparam file="$(find ccs)/config/wheels.yaml" command="load"/>

    <!-- Start the laser tilt controller -->
    <node name="charlie_wheels_spawner" pkg="ax12_controller_core" type="controller_spawner.py"
          args="start left_front_wheel_controller"
          output="screen"/>
</launch>


