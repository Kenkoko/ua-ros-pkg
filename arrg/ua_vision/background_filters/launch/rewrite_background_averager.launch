<launch>
    <node name="background_averager" pkg="background_filters" type="rewrite_background_sampler" output="screen" >
        <remap from="image" to="logitech_usb_webcam/image_raw" />
        <param name="scale" value="1" />
        <param name="colorspace" value="rgchroma" />
        <param name="number_of_samples" value="100" />
        <param name="sampling_delay" value="0.0" />
    </node>
</launch>
