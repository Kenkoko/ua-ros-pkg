<launch>
    <group ns="stereo">
        <node name="videre_stereo_node" pkg="videre_stereo_cam" type="videre_stereo_node" respawn="false" output="screen">
              <param name="videre_mode" type="str" value="none"/>
              <!--param name="videre_mode" type="str" value="disparity_raw"/-->
              <param name="calculate_points" type="bool" value="False"/>
              <!--param name="exposure" type="int" value="450"/-->
              <param name="exposure_auto" type="bool" value="True"/>
              <!--param name="brightness" type="int" value="50"/-->
              <param name="brightness_auto" type="bool" value="True"/>
              <!--param name="gain" type="int" value="10"/-->
              <param name="gain_auto" type="bool" value="True"/>
              <param name="frame_id" value="stereo_optical_frame"/>
        </node>
<!--        <node pkg="image_proc" type="image_proc" name="image_proc_left" ns="left" />-->
<!--        <node pkg="image_proc" type="image_proc" name="image_proc_right" ns="right" />-->
        <node pkg="stereo_image_proc" type="stereo_image_proc" name="stereo_image_proc" />
    </group>
</launch>


