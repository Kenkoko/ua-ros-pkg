<package>
  <description brief="phidgets">

     A python wrapper for the Phidgets Python API.

  </description>
  <author>UARobots</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/phidgets</url>
  <depend package="rospy"/>
  <depend package="std_msgs"/>
  <!-- just to run makefile for zip download --> 
  <depend package="phidgets_py_api"/>
  <export>
    <!-- depend will not use proper path for third-party py code -->
    <python path="${prefix}/../phidgets_py_api/src/phidgets_py_api"/>
  </export>
</package>


